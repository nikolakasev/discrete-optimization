var 1..9: C;
var 0..9: A;
var 0..9: O;
var 1..9: Z;
var 0..9: H;
var 0..9: I;
var 1..9: P;
var 1..9: T;
var 0..9: E;
var 0..9: S;

include "alldifferent.mzn";

constraint 2 * (C * 100 + A * 10 + O) + 100 * Z + H * 10 + I + 10 * P + I = 1000 * T + 100 * E + 10 * S + T;

constraint P = 3 * Z;

constraint O = 0;

constraint alldifferent([C, A, O, Z, H, I, P, T, E, S]);
    
solve satisfy;